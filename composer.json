{
    "name": "hiqdev/hidev-nginx",
    "type": "yii2-extension",
    "description": "HiDev Nginx plugin",
    "keywords": [
        "hidev",
        "nginx"
    ],
    "homepage": "https://github.com/hiqdev/hidev-nginx",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/hidev-nginx",
        "issues": "https://github.com/hiqdev/hidev-nginx/issues",
        "wiki": "https://github.com/hiqdev/hidev-nginx/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "hiqdev/composer-config-plugin": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev": "dev-master",
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hidev\\nginx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hidev\\nginx\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "hidev": "src/config/hidev.php",
            "tests": "src/config/tests.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
